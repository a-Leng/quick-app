{
  "package": "com.application.demo",
  "name": "myApp01",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1020,
  "icon": "/Common/logo.png",
  "deviceTypeList": [
    "phone"
  ],
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name":"system.geolocation"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "Index",
    "pages": {
      "Index": {
        "component": "index"
      },
      "shopDetail": {
        "component": "index"
      },
      "MapDetail":{
        "component":"index"
      },
      "About": {
        "component": "index"
      }
    },
    "widgets": {
      "CardDemo": {
        "name": "CardDemo",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/CardDemo",
        "features": []
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#000",
    "titleBarTextColor": "#fff",
    "pages": {
      "Index": {
        "titleBarText": "五元洗车",
        "menu": false
      },
      "shopDetail": {
        "titleBarText": "店铺详情"
      },
      "MapDetail":{
        "fullScreen":true,
        "titleBar":false
      },
      "About": {
        "menu": false,
        "fullScreen":true
      }
    }
  }
}